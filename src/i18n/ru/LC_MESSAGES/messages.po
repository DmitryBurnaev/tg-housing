#: src/handlers/bot_handlers.py:36
msgid "What do you want?"
msgstr "Что именно необходимо?"

#: src/handlers/bot_handlers.py:40
msgid "Add Address"
msgstr "Добавить адрес"

#: src/handlers/bot_handlers.py:41
msgid "Remove Address"
msgstr "Удалить адрес"

#: src/handlers/bot_handlers.py:64
msgid "Ok, Sent me your address (without city, default - SPB)"
msgstr "Ок, отправь мне свой адрес (без города)"

#: src/handlers/bot_handlers.py:87
msgid "What address do you want to remove?"
msgstr "Какой адрес ты хочешь убрать?"

#: src/handlers/bot_handlers.py:118
#, python-brace-format
msgid "Ups... we can't parse address \"{message_text} (got {new_address})\"."
msgstr ""

#: src/handlers/bot_handlers.py:135
#, python-brace-format
msgid "Ok, I'll remember your new address \"{new_address}\"."
msgstr "Ок, я запомню твой новый адрес"

#: src/handlers/bot_handlers.py:164
#, python-brace-format
msgid "OK. Address \"{message.text}\" was removed!"
msgstr "Ок, адрес \"{message.text}\" я удалил!"

#: src/handlers/bot_handlers.py:182
msgid "Cancelled."
msgstr "Отменено."

#: src/handlers/bot_handlers.py:198
#, python-brace-format
msgid "Hi, {full_name}!"
msgstr "Привет, {full_name}!"

#: src/handlers/bot_handlers.py:222
#, python-brace-format
msgid "Ok, I forgot all for you, {full_name}!"
msgstr "Ок, я забыл все о тебе, {full_name}!"

#: src/handlers/bot_handlers.py:245
msgid "Ok, That's your information:"
msgstr "Ок, вот твоя информация:"

#: src/handlers/helpers.py:41
msgid "Your Addresses:"
msgstr "Твои адреса:"

#: src/handlers/helpers.py:43 src/handlers/helpers.py:48
msgid "No address yet :("
msgstr "Нет пока добавленных адресов :("

#: src/handlers/helpers.py:52
msgid "No shutdowns :)"
msgstr "Не нашел никаких отключений в ближайшем будущем :)"

#: src/handlers/helpers.py:65
msgid "Start"
msgstr "Начало"

#: src/handlers/helpers.py:66
msgid "End"
